<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <data>
    <variable
        name="hide_profie"
        type="String" />
  </data>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:layout_marginLeft="37dp"
        android:textColor="@color/black"
        android:text="TỜ KHAI BÁO CỦA BẠN"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="360dp"
        android:background="#507AC3"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="-377dp"
        >




    <TextView
        android:id="@+id/lichtrinh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:text="lịch trình di chuyển trong 24h"/>
    <TextView
        android:id="@+id/lichtrinh1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"

        />
    <TextView
        android:id="@+id/hoten"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:text="Họ tên"/>
    <TextView
        android:id="@+id/hoten1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    <TextView
        android:id="@+id/namsinh"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textStyle="bold"
        android:text="Năm sinh"/>
    <TextView
        android:id="@+id/namsinh1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    <TextView
        android:id="@+id/gioitinh"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="18sp"
        android:text="Giới tính"/>
    <TextView
        android:id="@+id/gioitinh1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    <TextView
        android:id="@+id/quoctinh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:text="Quốc Tịch"/>
    <TextView
        android:id="@+id/quoctinh1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    <TextView
        android:id="@+id/sohochieu_giaythonghanh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:text="Số hộ chiếu hoặc giấy thông hành hợp pháp khác"/>
    <TextView
        android:id="@+id/sohochieu_giaythonghanh1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    <TextView
        android:id="@+id/tiepxuc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        android:text="Có tiếp xúc vs người F1, F2, F3 không"/>
    <TextView
        android:id="@+id/tiepxuc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17sp"
        />
    </LinearLayout>
    <Button
        android:id="@+id/gotothesecret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      android:text="nút bí mật (có thể bấm vào được)"
        android:layout_marginLeft="30dp"
        />
    <Button
        android:id="@+id/fastback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      android:backgroundTint="#0254F4"

      android:text="Làm lại Tờ khai báo"/>




  </LinearLayout>
</layout>
